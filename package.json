{
  "name": "messengerbank-metals-timelock",
  "version": "1.0.0",
  "description": "Smart Contracts for the MessengerBank Metals Token (MBM).",
  "files": [
    "contracts",
    "test"
  ],
  "keywords": [
    "solidity",
    "ethereum",
    "smart",
    "contracts",
    "token",
    "truffle",
    "openzeppelin"
  ],
  "scripts": {
    "dev": "lite-server",
    "console": "truffle develop",
    "compile": "truffle compile --all",
    "coverage": "scripts/coverage.sh",
    "clean": "rm -rf coverage",
    "profile": "npm run clean && npm run coverage && open coverage/index.html",
    "test": "npm run compile && scripts/test.sh",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:fix": "npm run lint:js:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "flat": "scripts/flat.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vittominacori/messengerbank-metals.git"
  },
  "homepage": "https://github.com/vittominacori/messengerbank-metals/",
  "bugs": {
    "url": "https://github.com/vittominacori/messengerbank-metals/issues"
  },
  "author": "Vittorio Minacori (https://github.com/vittominacori)",
  "contributors": [
    {
      "name": "Vittorio Minacori",
      "url": "https://github.com/vittominacori"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "ethereumjs-util": "^6.1.0",
    "ethjs-abi": "^0.2.1",
    "ganache-cli": "^6.4.1",
    "ganache-cli-coverage": "github:Agusx1211/ganache-cli#c462b3fc48fe9b16756f7799885c0741114d9ed3",
    "lite-server": "^2.4.0",
    "openzeppelin-test-helpers": "^0.3.0",
    "pify": "^4.0.1",
    "solhint": "^1.5.1",
    "solidity-coverage": "github:rotcivegaf/solidity-coverage#5875f5b7bc74d447f3312c9c0e9fc7814b482477",
    "surya": "^0.2.8",
    "truffle": "^5.0.9",
    "truffle-flattener": "1.2.12"
  },
  "dependencies": {
    "eth-token-recover": "2.2.1",
    "openzeppelin-solidity": "2.2.0"
  }
}
